    <div class="widget general-html none hide widget-none" id="73fe08f7-99bd-4253-a812-25baf62e3500"  >
        
        
            
        

        <div class="wrapped " id='awesome-audio-player'>
            
            <div class="widget-body body body-none "><!-- Fred's Awesome Audio Player v1.6 -->
<!-- 1.6 - Added ES&T to the list of podcasts -->
<!-- 1.5 - Sends elapsed play time on leaving page. References to elements are more specific. Removed Play_Podcast start time value. -->
<!-- 1.4 - Populates links with Google Analytics tracking on page load. Rearranged functions. -->
<!-- 1.3 - Commented out Google Analytics tracking on slider actions -->
<!-- 1.2 - Removed Google Analytics key value labels -->
<!-- 1.1 - Added Google Analytics key value labels -->
<!-- 1.0 - Allows for Google Analytics event tracking on loading the audio and play/pause/slider interaction. Play/pause/slider events also send the time position of the audio that is being interacted with. -->

<!-- Allows slider to work on touchscreens -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui-touch-punch/0.2.3/jquery.ui.touch-punch.min.js"></script>

<script type="text/javascript">
var windowUnload = false;

// Set tracking name based on journal coden, otherwise based on title tag if not on journal template
var currentJournal = "";
var journalTrackingName = "";

$(document).ready( function() { 
	currentJournal = $('#journalID').text();
	currentTitle = $('title').text();
	if (currentJournal == 'acbcct') {
		journalTrackingName = 'ACS.Chemical.Bio.Pod';
	} else if (currentJournal == 'asbcd6') {
		journalTrackingName = 'ACS.Synthetic.Bio.Pod';
	} else if (currentJournal == 'ancham') {
		journalTrackingName = 'Analytical.Chem.Pod';
	} else if (currentJournal == 'ancac3') {
		journalTrackingName = 'ACS.Nano.Pod';
	} else if (currentJournal == 'esthag') {
		journalTrackingName = 'EST.Pod';
	} else if (currentJournal == 'jacsat') {
		journalTrackingName = 'JACS.Pod';
	} else if (currentTitle == 'ACS Polymer Science Podcast Series') {
		journalTrackingName = 'Polymer.Pod';
	} else if (currentTitle == 'Test Page') {
		journalTrackingName = 'Test.Pod';
	}
});

var startAudioTime = 0;

// Populate links with Google Analytics tracking 
$(document).ready( function() { 
	$('.audioBoxWrap').each(function() {
		dataDate = $(this).children('.audioplayer').data('date');
		articleCount = 0;

		if($(this).children('.article')) {
			$(this).children('.article').each(function() {
				articleCount ++;
				$(this).children('a').attr("onclick", "ga('send', 'event', '" + journalTrackingName + "', 'Podcast_Article" + articleCount + "', '" + dataDate + "');");
			});
		}
		if($(this).children('.description').children('a')) {
			$(this).children('.description').children('a').each(function() {
				articleCount ++;
				$(this).attr("onclick", "ga('send', 'event', '" + journalTrackingName + "', 'Podcast_Article" + articleCount + "', '" + dataDate + "');");
			});
		}
		// $(this).find('.loadAudioPlayer').attr("onclick", "ga('send', 'event', '" + journalTrackingName + "', 'Load_Podcast', '" + dataDate + "');");
	});


	// Start Audio Player
	$(".loadAudioPlayer").click(function() {
		dataDate = $(this).parent('.audioplayer').data('date');
		ga("send", "event", journalTrackingName , "Load_Podcast", dataDate);

		$(this).hide();
		container = $(this).parent('.audioplayer');
		container.css('width', '350px');
		container.addClass('loading');
		playAudio = container.children('.play');
		pauseAudio = container.children('.pause');
		var audioDuration = 0;
		var currentRunTime = "00:00";

		// set audio source, type, and volume
		container.children('.message').show();
		audioFile = new Audio();
		container.data('name', audioFile);
		audioFile.src = container.data('file');
		audioFile.type = 'audio/mpeg';
		//audioFile.src = container.data('file').split('.mp3')[0] + '.ogg';
		//audioFile.type = 'audio/ogg';
		// set audio to max volume
		audioFile.volume = 1;

		loadAudioTimeout();
		// set current time to 00:00, total duration, and create slider on audio player
		audioFile.addEventListener("loadedmetadata", function(_event) {
			audioDuration = parseInt(audioFile.duration, 10);
			sec = Math.floor( audioDuration );    
			min = Math.floor( sec / 60 );
			min = min >= 10 ? min : '0' + min;    
			sec = Math.floor( sec % 60 );
			sec = sec >= 10 ? sec : '0' + sec;    
			durationMinSec = min + ':' + sec;
			container.children('.runTime').children('.totalDuration').html(" / " + durationMinSec);
			container.children('.runTime').children('.currentTime').html('00:00');	
			container.children('.message').hide();
			container.children('.play').show();
			container.removeClass('loading');
			container.addClass('loaded');
			createSlider();
		});
		
		// create audio player slider - max is audio duration in seconds.
		audioSlider = container.children('.slider');
		function createSlider() {
			audioSlider.slider({
				range : false,
				value: 0,
				step: 1,
				orientation: "horizontal",
				animate: "true",
				min: 0,
				max: audioDuration,
				slide: function( event, ui ) {
					$(this).parent('.audioplayer').data('name').currentTime = ui.value;
				},
				stop: function( event, ui ) {
					//update current run time after moving slider
					$(this).parent('.audioplayer').data('name').currentTime = ui.value;
					currentRunTime = calculateRunTime(ui.value);
					$(this).parent('.audioplayer').children('.runTime').children('.currentTime').html(currentRunTime);
					if($(this).parent('.audioplayer').hasClass('active')) {
						startAudioTime = parseInt($(this).parent('.audioplayer').data('name').currentTime);
						// ga('send', 'event', journalTrackingName, 'SliderAudioActive', $(this).parent('div').data('date'));
						// ga('send', 'event', journalTrackingName, 'SliderAudioActive', $(this).parent('div').data('date'), parseInt(ui.value)); // disable sending time on this action
					}
					// else {
						// ga('send', 'event', journalTrackingName, 'SliderAudioInactive', $(this).parent('div').data('date'));
						// ga('send', 'event', journalTrackingName, 'SliderAudioInactive', $(this).parent('div').data('date'), parseInt(ui.value)); // disable sending time on this action
					// }
				}
			});
		};
		// calculates elapsed time when interacting with slidering during playback
		audioSlider.bind('mousedown', function(e) {
			if($(this).parent('.audioplayer').hasClass('active')) {
				listenedDuration = $(audioSlider).slider('value') - startAudioTime;
				newtime = parseInt($(this).parent('.audioplayer').attr('data-time')) + listenedDuration;
				$(this).parent('.audioplayer').attr('data-time', newtime);
			}
		});

		// audio player play button
		playAudio.bind('click', function(e) {
			e.preventDefault();
			// check for other active audio and pause
			$('.audioplayer.active').each(function(){
				ga('send', 'event', journalTrackingName, 'Paused_By_Other_Podcast', $('.audioplayer.active').data('date'), parseInt($('.audioplayer.active').data('name').currentTime));
				listenedDuration = parseInt($('.audioplayer.active').data('name').currentTime - startAudioTime);
				newtime = parseInt($('.audioplayer.active').attr('data-time')) + listenedDuration;
				$('.audioplayer.active').attr('data-time', newtime);
				// ga('send', 'event', journalTrackingName, 'Paused_By_Other_Audio_Elapsed', $('.audioplayer.active').data('date'), listenedDuration);
				$(this).removeClass('active');
				$(this).data('name').pause();
				$(this).children('.pause').hide();
				$(this).children('.play').show();
			});
			//play audio and make pause button available
			$(this).parent('.audioplayer').addClass('active');
			$(this).parent('.audioplayer').data('name').play();
			startAudioTime = parseInt($(this).parent('.audioplayer').data('name').currentTime);
			$(this).hide();
			$(this).parent('.audioplayer').children('.pause').show();

			if(!($(this).hasClass('played'))) {
				ga('send', 'event', journalTrackingName, 'Play_Podcast', $(this).parent('.audioplayer').data('date'));
				$(this).addClass('played');
				if(!$(this).parent('.audioplayer').attr('data-time')) {
					$(this).parent('.audioplayer').attr('data-time', 0);
				}
			}

			// continually calculate and update current runtime
			currentAudio = $(this).parent('.audioplayer');
			currentAudio.data('name').addEventListener("timeupdate", function(_event) {
				currentRunTime = $(this)[0].currentTime;
				sec = Math.floor( currentRunTime );    
				min = Math.floor( sec / 60 );
				min = min >= 10 ? min : '0' + min;    
				sec = Math.floor( sec % 60 );
				sec = sec >= 10 ? sec : '0' + sec;    
				currentRunTime = min + ':' + sec;
				currentAudio.children('.runTime').children('.currentTime').html(currentRunTime);

				if(currentAudio.hasClass('active')) {
					if ( (parseInt($('.audioplayer.active').data('name').currentTime) >= (parseInt($('.audioplayer.active').data('name').duration) - 15)) && !($('.audioplayer.active').hasClass('completed')) ) {
						$('.audioplayer.active').addClass('completed');
						listenedDuration = parseInt($('.audioplayer.active').data('name').currentTime) - startAudioTime;
						ga('send', 'event', journalTrackingName, 'Finished_Podcast', $('.audioplayer.active').data('date'), parseInt($('.audioplayer.active').data('name').currentTime));
						// ga('send', 'event', journalTrackingName, 'Finished_Podcast_Elapsed', $('.audioplayer.active').data('date'), listenedDuration);
					}
				}
			});
			// updates slider with current runtime
			updateSlider(currentAudio);
		});

		// audio player pause button
		pauseAudio.bind('click', function(e) {
			e.preventDefault();
			$(this).parent('.audioplayer').removeClass('active');
			$(this).parent('.audioplayer').data('name').pause();
			$(this).hide();
			$(this).parent('.audioplayer').children('.play').show();
			listenedDuration = (parseInt($(this).parent('.audioplayer').data('name').currentTime) - startAudioTime);
			newtime = parseInt($(this).parent('.audioplayer').attr('data-time')) + listenedDuration;
			$(this).parent('.audioplayer').attr('data-time', newtime);
			// ga('send', 'event', journalTrackingName, 'Pause_Podcast', $(this).parent('div').data('date'), parseInt($(this).parent('div').data('name').currentTime));
			// ga('send', 'event', journalTrackingName, 'Pause_Podcast_Elapsed', $(this).parent('div').data('date'), listenedDuration);
		});
		
		// timeout hides play options and slider if audio does not load after 30 seconds
		function loadAudioTimeout() {
			setTimeout(function() {
				if ($('.audioplayer').hasClass('loading')){
					ga('send', 'event', journalTrackingName, 'Failed_to_load_Podcast', $('.audioplayer.loading').data('date'));
				}
				$('.audioplayer.loading').addClass('loadingError');
				$('.audioplayer.loading').removeClass('loading');
				$('.audioplayer.loadingError').children('.message').html("Please check your connection and verify your browser is up to date");
				$('.audioplayer.loadingError').children('.message').css({'left' : '66px', 'width' : '220px'});
				$('.audioplayer.loadingError').css('height', '45px');
				$('.audioplayer.loadingError').children('.play').hide();
				$('.audioplayer.loadingError').children('.slider').hide();
				$('.audioplayer.loadingError').children('.runTime').hide();
			}, 60000);
		};

		// calculates time code, used to update time after for slider interaction
		function calculateRunTime (number) {
			var time = number;
			sec = Math.floor( time );    
			min = Math.floor( sec / 60 );
			min = min >= 10 ? min : '0' + min;    
			sec = Math.floor( sec % 60 );
			sec = sec >= 10 ? sec : '0' + sec;    
			time = min + ':' + sec;
			return time;
		};
		
		// update slider with current runtime during play
		function updateSlider (currentRun) {
			currentRun.data('name').addEventListener('timeupdate',function (){
				if($(currentRun).hasClass('active')) {
					curtime = parseInt(currentRun.data('name').currentTime, 10);
					$(currentRun).children('.slider').slider( "value", curtime );
					if(parseInt(currentRun.data('name').currentTime) == parseInt(currentRun.data('name').duration)) {
						$(currentRun).removeClass('active');
					}
				}
			});
		};
	});
});

$(window).on('beforeunload', function() {
	if(windowUnload == false) {
		windowUnload = true;
		$('.audioplayer.active').each(function() {
			listenedDuration = parseInt($('.audioplayer.active').data('name').currentTime - startAudioTime);
			newtime = parseInt($('.audioplayer.active').attr('data-time')) + listenedDuration;
			$('.audioplayer.active').attr('data-time', newtime);
			$('.audioplayer.active').data('name').pause();
			$('.audioplayer.active').removeClass('active');
		});
	    $.each($('.played'), function() {
	    	$(this).parent('.audioplayer').remove();
	    	ga('send', 'event', journalTrackingName, 'Podcast_Elapsed_Time', $(this).parent('.audioplayer').data('date'), $(this).parent('.audioplayer').attr('data-time'));
	    });
	}
});

</script>

<div id="journalID" style="display: none;">${journal.primaryCode}</div>




<style type="text/css">
/* Old audioBoxWrap styles to remove */
.audioBoxWrap img.cover {
  float: right;
  margin-left: 7px;
  margin-right: 7px;
  border: 1px solid #ccc;
}
.audioBoxWrap div {
  clear: left;
  margin-left: 0;
  font-size: 1em;
}
.audioBoxWrap p {
  margin-bottom: 0;
}
.audioBoxWrap .info {
  padding: 2px 0;
}

/* Updated audioBoxWrap styles */
.audioBoxWrap {
  padding-top: 0; /* remove later */
  padding-bottom: 0; /* remove later */
  border: none; /* remove later */
  margin: 0 0 35px 0;
  width: 500px;
  font-size: 1rem;
}
.audioBoxWrap h2.title {
  margin-top: 0; /* remove later */
  color: #000;
  margin: 0 0 6px 0;
  font-size: 1.1em;
  font-weight: 600;
}
.audioBoxWrap h3.date {
  margin-bottom: 0; /* remove later */
  color: #000;
  margin: 0 0 6px 0;
  font-size: 0.9em;
  font-weight: 600;
}
.audioBoxWrap .duration {
	font-size: 0.9em;
	color: #666;
	margin: 0 0 14px 0;
	background: url('http://pubs.acs.org/pb-assets/images/audioplayer/audio-microphone.png') no-repeat;
	background-position: 0 0;
	padding-left: 20px;
}
.audioBoxWrap .description {
	font-size: 0.9em;
	color: #000;
	margin: 0 0 12px 0;
}
.audioBoxWrap .article {
	font-size: 0.9em;
	color: #000;
	margin: 0 0 6px 0;
	padding: 0;
}

/* Awesome Audioplayer styles  */
.audioBoxWrap .audioplayer {
	display: block;
	height: 30px;
	width: 150px;
